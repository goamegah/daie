{
	"name": "ds_github_csv",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_github_HttpServer",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"folderPath": {
				"type": "string",
				"defaultValue": "dataset/data/caract_v1"
			},
			"fileName": {
				"type": "string",
				"defaultValue": "caract.csv"
			}
		},
		"folder": {
			"name": "github/opendata"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "HttpServerLocation",
				"relativeUrl": "@{dataset().folderPath}/@{dataset().fileName}"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}