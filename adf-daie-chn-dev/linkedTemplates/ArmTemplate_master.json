{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-daie-chn-dev"
		},
		"ls_github_HttpServer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/goamegah/daie/"
		},
		"ls_key_vault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-daie-chn-dev-2025.vault.azure.net/"
		},
		"LS_ADLS_DataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stadaiechndev.dfs.core.windows.net/"
		},
		"LS_ADLS_DataLake_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "20225d8c-26c2-42d8-becc-f275b34b7356"
		},
		"LS_ADLS_DataLake_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "95a69785-5397-4c66-91c2-1541f7a1eda8"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-daie-chn-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_github_HttpServer_properties_typeProperties_url": {
						"value": "[parameters('ls_github_HttpServer_properties_typeProperties_url')]"
					},
					"ls_key_vault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_key_vault_properties_typeProperties_baseUrl')]"
					},
					"LS_ADLS_DataLake_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_DataLake_properties_typeProperties_url')]"
					},
					"LS_ADLS_DataLake_properties_typeProperties_tenant": {
						"value": "[parameters('LS_ADLS_DataLake_properties_typeProperties_tenant')]"
					},
					"LS_ADLS_DataLake_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('LS_ADLS_DataLake_properties_typeProperties_servicePrincipalId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}